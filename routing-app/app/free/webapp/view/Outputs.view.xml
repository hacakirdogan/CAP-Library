<mvc:View
    controllerName="free.controller.Main"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:l="sap.ui.layout"
    xmlns:vbm="sap.ui.vbm"
    xmlns:html="http://www.w3.org/1999/xhtml"
    displayBlock="true"
>
    <NavContainer
        navigationFinished="onNavigationFinished"
        id="navConII"
        width="auto"
        height="35em"
        class="navContainerControl sapUiSmallMarginBottom"
    >
        <Page
            id="p3"
            title="Route"
        >
            <headerContent>
                <Button
                    type="Emphasized"
                    text="List"
                    press="handleNavII"
                >
                    <customData>
                        <core:CustomData
                            key="target"
                            value="p4"
                        />
                    </customData>
                </Button>
            </headerContent>
            <html:iframe
                src="./route.html"
                height="98%"
                width="100%"
            />
        </Page>
        <Page
            id="p4"
            title="Route"
        >
            <headerContent>
                <Button
                    type="Emphasized"
                    text="Map"
                    press="handleNavII"
                >
                    <customData>
                        <core:CustomData
                            key="target"
                            value="p3"
                        />
                    </customData>
                </Button>
            </headerContent>
            <Text text="Hello" />
        </Page>
    </NavContainer>
</mvc:View>
